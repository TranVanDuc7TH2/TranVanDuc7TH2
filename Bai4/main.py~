from khoa import Khoa
from sinhvien import SinhVien

print ("----DANH SACH KHOA ----")
khoa1 = Khoa("01","CNTT")
khoa2 = Khoa("02","Ke Toan")
khoa3 = Khoa("03","Co Khi")
khoa4 = Khoa("04","Nuoi trong thuy san")

khoa1.hienThi()
khoa2.hienThi()
khoa3.hienThi()
khoa4.hienThi()

print("----DANH SACH SINH VIEN----")
sv1 = SinhVien("001","Mai A","01")
sv2 = SinhVien("002","Tran B","01")
sv3 = SinhVien("003","Van C","02")
sv4 = SinhVien("004","Thi K","01")

sv1.hienThi()
sv2.hienThi()
sv3.hienThi()
sv4.hienThi()





